(function (angular) {
    "use strict";

    angular.module("app")
        .run(run);

    run.$inject = [
        "$templateCache"
    ];

    function htmlDecode(value) {
        return angular.element("<div/>")
            .html(value).text();
    }

    function run($templateCache) {<% templates.forEach(function (template) { %>
        $templateCache.put("<%- template.file %>", htmlDecode("<%= template.contents %>"));<% }); %>
    }
})(angular);
